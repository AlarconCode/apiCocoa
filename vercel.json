{
  "version": 2,
  "builds": [
      {
          "src": "./index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js",
        "methods": ["GET,OPTIONS,PATCH,DELETE,POST,PUT"],
        "headers": {
          "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
          "Access-Control-Allow-Headers": "Content-Type"
        }
      }
  ]
}